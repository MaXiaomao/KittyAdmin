{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true
	},
	"extends": ["plugin:react/recommended", "airbnb", "plugin:prettier/recommended"],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		"func-names": 0,
		"no-console": "off",
		"react/react-in-jsx-scope": "off",
		"import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
	}
}
